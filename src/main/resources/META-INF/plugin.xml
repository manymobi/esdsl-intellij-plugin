<idea-plugin>
	<id>com.manymobi.jetbrains.esdsl</id>
	<name>Esdsl</name>
	<vendor email="631583871@qq.com" url="https://github.com/manymobi/esdsl-jetbrains-plugin">
		Esdsl Jetbrains Plugin
	</vendor>
	
	<description><![CDATA[
  <p>Esdsl plugin</p>

<p>
<a href="https://github.com/manymobi/esdsl-jetbrains-plugin">Github source</a>
<a href="https://github.com/manymobi/esdsl-core">Esdsl</a>
<a href="https://github.com/manymobi/esdsl-spring">Esdsl Spring</a>
  ]]></description>
	
	<change-notes><![CDATA[
        ]]>
	</change-notes>
	
	
	<!-- please see http://confluence.jetbrains.net/display/IDEADEV/Plugin+Compatibility+with+IntelliJ+Platform+Products
		 on how to target different products -->
	<!-- uncomment to enable plugin in all products -->
	<depends>com.intellij.modules.lang</depends>
	<depends>com.intellij.modules.java</depends>
	<depends>com.intellij.modules.platform</depends>
	
	<application-components>
	</application-components>
	
	<actions>
	</actions>
	
	<extensions defaultExtensionNs="com.intellij">
		<fileTypeFactory implementation="com.manymobi.jetbrains.esdsl.EsdslFileTypeFactory"/>
		<lang.syntaxHighlighterFactory language="Esdsl"
		                               implementationClass="com.manymobi.jetbrains.esdsl.EsdslSyntaxHighlighterFactory"/>
		
		<lang.parserDefinition language="Esdsl"
		                       implementationClass="com.manymobi.jetbrains.esdsl.EsdslParserDefinition"/>
		<lang.ast.factory language="Esdsl" implementationClass="com.manymobi.jetbrains.esdsl.SampleASTFactory"/>
		
		<lang.psiStructureViewFactory language="Esdsl"
		                              implementationClass="com.manymobi.jetbrains.esdsl.structview.EsdslStructureViewFactory"/>
		
		<colorSettingsPage implementation="com.manymobi.jetbrains.esdsl.EsdslColorSettingsPage"/>
		
		<!--		<externalAnnotator language="Esdsl" implementationClass="com.manymobi.jetbrains.esdsl.SampleExternalAnnotator"/>-->
		
		<!--		<lang.findUsagesProvider language="Esdsl"-->
		<!--		                         implementationClass="com.manymobi.jetbrains.esdsl.SampleFindUsagesProvider"/>-->
		<lang.commenter language="Esdsl" implementationClass="com.manymobi.jetbrains.esdsl.EsdslCommenter"/>
		<lang.braceMatcher language="Esdsl"
		                   implementationClass="com.manymobi.jetbrains.esdsl.EsdslPairedBraceMatcher"/>
		<!--		方法连接-->
		<codeInsight.lineMarkerProvider language="JAVA"
		                                implementationClass="com.manymobi.jetbrains.esdsl.line.JavaEsdslLineMarkerProvider"/>
		<codeInsight.lineMarkerProvider language="Esdsl"
		                                implementationClass="com.manymobi.jetbrains.esdsl.line.EsdslLineMarkerProvider"/>
		<!--		代码格式化-->
		<!--		<lang.formatter language="Esdsl"-->
		<!--		                implementationClass="com.manymobi.jetbrains.esdsl.formatting.EsdslFormattingModelBuilder"/>-->
		<!--		<codeStyleSettingsProvider implementation="com.manymobi.jetbrains.esdsl.formatting.EsdslCodeStyleSettingsProvider"/>-->
		<!--		<langCodeStyleSettingsProvider implementation="com.manymobi.jetbrains.esdsl.formatting.EsdslLanguageCodeStyleSettingsProvider"/>-->
	</extensions>
</idea-plugin>
